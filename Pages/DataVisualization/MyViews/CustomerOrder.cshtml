@page
@model ProjectObryckiJustin.Pages.DataVisualization.MyViews.CustomerOrderModel
@{
    ViewData["Title"] = "Customer Orders";
    Layout = "~/Pages/Shared/Views_Layout.cshtml";
}

<h1> Customer Order's </h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].PartName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].PartDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].TotalCost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VwCustomerOrder[0].OrderDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.VwCustomerOrder)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PartName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PartDescription)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalCost)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OrderDate)
                </td>
            </tr>
        }
    </tbody>
</table>
